Sub Process_Globals
	'These global variables will be declared once when the application starts.
	'Public variables can be accessed from all modules.
	Public App As Application
	Public NavControl As NavigationController
	Private Page1 As Page
	Private lblLeft As Label
	Private lblRight As Label
	Private iRangeBar1 As iRangeBar
	Private lblLeft2 As Label
	Private lblRight2 As Label
	Private iRangeBar2 As iRangeBar
	Private lblLeft3 As Label
	Private lblRight3 As Label
	Private iRangeBar3 As iRangeBar
End Sub

Private Sub Application_Start (Nav As NavigationController)
	'SetDebugAutoFlushLogs(True) 'Uncomment if program crashes before all logs are printed.
	NavControl = Nav
	Page1.Initialize("Page1")
	Page1.Title = "Page 1"
	Page1.RootPanel.Color = Colors.White
	Page1.RootPanel.LoadLayout("main")
	iRangeBar1.assignrangeImage(LoadBitmap(File.DirAssets, "bar-background.png"))
	iRangeBar1.assignleftThumb(LoadBitmap(File.DirAssets, "handle.png"))
	iRangeBar1.assignrightThumb(LoadBitmap(File.DirAssets, "handle.png"))
	iRangeBar1.assigntrackImage(LoadBitmap(File.DirAssets, "bar-highlight.png"))
	iRangeBar1.setMinValue(0, 100)
	iRangeBar1.setMinimumDistance(30)
	
	'''second rangebar
	iRangeBar2.assignrangeImage(LoadBitmap(File.DirAssets, "slider-default7-trackbackground@2x.png"))
	iRangeBar2.assignleftThumb(LoadBitmap(File.DirAssets, "slider-metal-handle@2x.png"))
	iRangeBar2.assignrightThumb(LoadBitmap(File.DirAssets, "slider-metal-handle@2x.png"))
	iRangeBar2.assigntrackImage(LoadBitmap(File.DirAssets, "slider-default-trackcrossedover@2x.png"))
	iRangeBar2.setMinValue(-100, 100)
	iRangeBar2.setMinimumDistance(30)
	
	'''third rangebar
	iRangeBar3.assignrangeImage(LoadBitmap(File.DirAssets, "slider-default7-trackbackground@2x.png"))
	iRangeBar3.assignleftThumb(LoadBitmap(File.DirAssets, "slider-metal-handle@2x.png"))
	iRangeBar3.assignrightThumb(LoadBitmap(File.DirAssets, "slider-metal-handle@2x.png"))
	iRangeBar3.assigntrackImage(LoadBitmap(File.DirAssets, "slider-default7-trackcrossedover@2x.png"))
	iRangeBar3.setMinValue(-80, 180)
	iRangeBar3.setMinimumDistance(20)
	NavControl.ShowPage(Page1)
End Sub

