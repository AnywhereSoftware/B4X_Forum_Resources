Sub iRangeBar1_SliderValueChanged (leftValue As Double, rightValue As Double)
	lblLeft.Text = NumberFormat(leftValue, 0, 2)
	lblRight.Text = NumberFormat(rightValue, 0, 2)
End Sub

Sub iRangeBar2_SliderValueChanged (leftValue As Double, rightValue As Double)
	lblLeft2.Text = NumberFormat(leftValue, 0, 2)
	lblRight2.Text = NumberFormat(rightValue, 0, 2)
End Sub

