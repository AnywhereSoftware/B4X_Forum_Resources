Private Sub AddCode(symbol As String, htmlCode As String, CSScode As String, unicode As String, entity As String, name As String, group As String)

	Dim t1 As wmHtmlCode
	Dim i As Int = listHtmlCodes.Size
	Dim s As String

	t1.Initialize
	t1.id = i
	t1.symbol = symbol
	t1.htmlCode = htmlCode
	t1.CSScode = CSScode
	t1.unicode = unicode
	t1.entity = entity
	t1.name = name
	t1.group = group

	listHtmlCodes.Add(t1)

	mapEntity.Put(entity, t1.id)
	mapCSScode.Put(CSScode, t1.id)
	mapHtmlCode.Put(htmlCode, t1.id)
	mapName.Put(name, t1.id)
	mapSymbol.Put(symbol, t1.id)
	mapUnicode.Put(unicode, t1.id)

	If i > 0 Then
		s = entity.SubString2(1, entity.Length - 1) ' For entity "&quot;", this would be "quot"
		xmlEntities.Put(s, symbol)
		matcherPattern = matcherPattern & "|" & s
	End If

End Sub

