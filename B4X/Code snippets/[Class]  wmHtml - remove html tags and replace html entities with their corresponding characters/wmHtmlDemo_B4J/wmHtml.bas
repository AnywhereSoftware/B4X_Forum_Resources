' Decodes an html string and replaces the found encoded entities with their symbols (characters).
Public Sub DecodeHtml(encoded As String) As String

	' Code based on https://www.b4x.com/android/forum/threads/resolved-i-have-some-special-char-in-the-text-how-to-make-it-normal-html-decoder.58856/#post-370577

	Dim sb As StringBuilder
	sb.Initialize
	' Dim xmlEntities As Map = CreateMap("quot": QUOTE, "lt": "<", "gt": ">", "amp": "&") ' Original code
	' Dim m As Matcher = Regex.Matcher("&(#\d+|quot|lt|gt|amp);", encoded) ' Original code
	Dim m As Matcher = Regex.Matcher(matcherPattern, encoded) ' My code; matcherPattern is set by Initialize and AddCode
	Dim index As Int
	Do While m.Find
		sb.Append(encoded.SubString2(index, m.GetStart(0)))
		index = m.GetEnd(0)
		If xmlEntities.ContainsKey(m.Group(1)) Then
			sb.Append(xmlEntities.Get(m.Group(1)))
		Else
			Dim rawCodepoint As String = m.Group(1).SubString(1)
			If IsNumber(rawCodepoint) Then
				sb.Append(Chr(rawCodepoint))
			Else
				sb.Append(m.Match)
			End If
		End If
	Loop
	If index < encoded.Length Then sb.Append(encoded.SubString(index))
	Return sb.ToString

End Sub

' Remove all html tags from the given string and return the result.
Public Sub RemoveHtmlTags(encoded As String) As String

	' Code based on
	' - https://www.b4x.com/android/forum/threads/resolved-i-have-some-special-char-in-the-text-how-to-make-it-normal-html-decoder.58856/#post-370577
	' - http://www.b4x.com/android/forum/threads/remove-html-formatting-from-a-string.34816/

	Dim sb As StringBuilder
	sb.Initialize
	Dim m As Matcher = Regex.Matcher2("<[^>]*>", Regex.CASE_INSENSITIVE, encoded)
	Dim index As Int
	Do While m.Find
		sb.Append(encoded.SubString2(index, m.GetStart(0)))
		index = m.GetEnd(0)
	Loop
	If index < encoded.Length Then sb.Append(encoded.SubString(index))
	Return sb.ToString

End Sub