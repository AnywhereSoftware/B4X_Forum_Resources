Sub AppStart (Form1 As Form, Args() As String)

	MainForm = Form1
	MainForm.RootPane.LoadLayout("Layout1")
	MainForm.Show
	
	Dim wraf As WavRandomAccessFile
	wraf.Initialize("Path to file","metadata.wav")						'Load the download example file metadata.zip
	Log(wraf.ChannelConfig)
	Log(wraf.SamplesizeInBits)
	Log(wraf.SampleRateInHz)
	
	
	Log("Availeble chunks")
	For Each K As String In wraf.WavHeaderChunkMap.Keys
		Dim C As WavHeaderType = wraf.WavHeaderChunkMap.Get(K)
		Log(K & " : " & C.StartPos & " : " & C.Length)
	Next
	
	Dim MR As Map
	MR.Initialize

	wraf.ReadListData(wraf.RandomAccessFile,"list-info",wraf.WavHeaderChunkMap,MR,"UTF-8")
	Log("Content for " & "list-info")
	For Each K As String In MR.Keys
		Log(K & " : " & MR.Get(K) & "|")
	Next

	wraf.Close



End Sub

