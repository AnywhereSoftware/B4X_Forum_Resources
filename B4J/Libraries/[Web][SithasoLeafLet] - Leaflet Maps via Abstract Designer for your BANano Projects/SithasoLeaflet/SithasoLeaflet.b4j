Sub Process_Globals
	Private BANano As BANano 'ignore
End Sub

Sub AppStart (Form1 As Form, Args() As String)	
	' The name of your library. Strongly suggested to let it begin with BANano!
	BANano.Initialize("BANano", "SithasoLeaflet", DateTime.Now)
	
	BANano.Header.AddJavascriptFile("leaflet.js")
	BANano.Header.AddCSSFile("leaflet.css")
	BANano.Header.AddJavascriptFile("leaflet.rotatedMarker.js")
	BANano.Header.AddCSSFile("Leaflet.Coordinates-0.1.5.css")
	BANano.Header.AddJavascriptFile("Leaflet.Coordinates-0.1.5.min.js")
			
	' start the build
	#if release
		BANano.BuildAsB4Xlib("1.00") 'version
	#else
		BANano.Build(File.DirApp)
	#end if
	
	ExitApplication
End Sub

'Return true to allow the default exceptions handler to handle the uncaught exception.
Sub Application_Error (Error As Exception, StackTrace As String) As Boolean
	Return True
End Sub

