<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="b4a.zsk.ExportRingtone"
	android:versionCode="1"
	android:versionName=""
	android:installLocation="internalOnly">
	
	<uses-sdk android:minSdkVersion="5" android:targetSdkVersion="29"/>
		<supports-screens android:largeScreens="true" 
	    				  android:normalScreens="true" 
	    				  android:smallScreens="true" 
	    				  android:anyDensity="true"/>
	<application
		android:icon="@drawable/icon"
		android:label="ExportRingtone"
		android:theme="@style/LightTheme">
		
		<activity android:name="b4a.zsk.FileList.FileListActivity" android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"/>
		<activity
			android:windowSoftInputMode="stateHidden"
			android:launchMode="singleTop"
			android:name=".main"
			android:label="ExportRingtone"
			android:screenOrientation="unspecified">
			<intent-filter>
			   <action android:name="android.intent.action.MAIN" />
			   <category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			
		</activity>
	</application>
</manifest>