		<receiver
			android:name=".smsgatewayservice$smsgatewayservice_BR"
			android:exported="true">
		</receiver>
		<service
			android:name=".starter"
			android:exported="true">
		</service>
		<receiver
			android:name=".starter$starter_BR"
			android:exported="true">
		</receiver>
		<receiver
			android:name=".httputils2service"
			android:exported="true">
		</receiver>
	</application>
</manifest>