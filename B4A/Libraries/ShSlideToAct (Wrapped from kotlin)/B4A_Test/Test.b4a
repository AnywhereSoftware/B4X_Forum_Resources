Sub Activity_Create(FirstTime As Boolean)
	Activity.LoadLayout("Layout")

	Dim bm As BitmapDrawable
	bm.Initialize(LoadBitmap(File.DirAssets, "Heart-icon.png"))
	
	ShSlideToAct1.Text = "سلام"
	ShSlideToAct1.AnimationDuration = 1000
	ShSlideToAct1.BorderRadius = 6dip
	ShSlideToAct1.ButtonMargin = 6dip
'	ShSlideToAct1.CompleteIcon = bm
	ShSlideToAct1.IconColor = Colors.Black
	ShSlideToAct1.InnerColor = Colors.White
	ShSlideToAct1.OuterColor = Colors.Black
	ShSlideToAct1.Rtl = True
'	ShSlideToAct1.RotateIcon = True
	ShSlideToAct1.Typface = Typeface.LoadFromAssets("BNaznnBd.ttf")
	ShSlideToAct1.TextColor = Colors.Blue
	ShSlideToAct1.TextSize = 25
	ShSlideToAct1.Vibration = 500
End Sub

