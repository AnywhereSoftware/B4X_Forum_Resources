Sub tabmaincontent_PageSelected (Position As Int)
	For i=0 To B4ASmoothBar1.getTotalTab(Position)-1
		If Position<>i Then
			B4ASmoothBar1.DeIndexTab=i
			Else
			B4ASmoothBar1.IndexTab=Position
		End If
	Next
End Sub

