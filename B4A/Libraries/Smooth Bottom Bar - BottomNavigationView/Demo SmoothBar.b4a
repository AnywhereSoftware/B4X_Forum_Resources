Sub Globals
	'These global variables will be redeclared each time the activity is created.
	Private B4ASmoothBar1 As B4ASmoothBar
	Private pnltabstrip As Panel
	Private tabmaincontent As TabStrip
	
End Sub

Sub Activity_Create(FirstTime As Boolean)
	Activity.LoadLayout("Layout")
	pnltabstrip.SetLayout(0,B4ASmoothBar1.Height,100%x,100%y-B4ASmoothBar1.Height)
	pnltabstrip.LoadLayout("frm_tabmaincontent")
	
	tabmaincontent.LoadLayout("frm_one","Form 1")
	tabmaincontent.LoadLayout("frm_two","Form 2")
	tabmaincontent.LoadLayout("frm_three","Form 3")
	
	B4ASmoothBar1.setListener
	
	ColorPatterm0
	
	
	Dim xml As XmlLayoutBuilder
	Dim dstab As List
	dstab.Initialize
	dstab.Add(CreateMap("icon":xml.GetResourceId("drawable","ic_home_white_24dp"),"title":"Home"))
	dstab.Add(CreateMap("icon":xml.GetResourceId("drawable","ic_history_white_24dp"),"title":"Recent"))
	dstab.Add(CreateMap("icon":xml.GetResourceId("drawable","ic_settings_white_24dp"),"title":"Settings"))
	B4ASmoothBar1.addTab(dstab)
	
	
End Sub


