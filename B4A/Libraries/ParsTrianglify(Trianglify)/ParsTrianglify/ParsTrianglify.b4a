End Sub

Sub Activity_Create(FirstTime As Boolean)

	PT.Initialize("")
	Activity.AddView(PT,0,0,-1,-1)



	Dim sb As SeekBar
	sb.Initialize("sb")
	Activity.AddView(sb,0,0,-1,-2)

	Dim sb1 As SeekBar
	sb1.Initialize("sb1")
	Activity.AddView(sb1,0,50dip,-1,-2)
	
	
	Dim sb2 As SeekBar
	sb2.Initialize("sb2")
	sb2.Max=27
	Activity.AddView(sb2,0,100dip,-1,-2)
	
	
	Dim sb3 As SeekBar
	sb3.Initialize("sb3")
	Activity.AddView(sb3,0,150dip,-1,-2)

End Sub
