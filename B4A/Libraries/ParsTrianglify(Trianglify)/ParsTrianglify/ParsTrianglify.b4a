	PT.ColorsPalette=Array As Int(Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255)),Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255)),Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255)),Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255)),Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255)),Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255)),Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255)),Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255)),Colors.RGB(Rnd(0,255),Rnd(0,255),Rnd(0,255)))
	PT.smartUpdate
End Sub
Sub sb2_ValueChanged (Value As Int, UserChanged As Boolean)
	PT.Palette=Value
	PT.smartUpdate
End Sub
Sub sb1_ValueChanged (Value As Int, UserChanged As Boolean)
	PT.setCellSize(Value+100)
	PT.smartUpdate
End Sub
Sub sb_ValueChanged (Value As Int, UserChanged As Boolean)
	PT.setVariance(Value)
	PT.smartUpdate
End Sub
