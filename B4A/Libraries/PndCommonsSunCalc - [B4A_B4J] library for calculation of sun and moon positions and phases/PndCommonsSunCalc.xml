            <parameter>
                <name>Month</name>
                <type>int</type>
            </parameter>
            <parameter>
                <name>Day</name>
                <type>int</type>
            </parameter>
            <parameter>
                <name>TwilightConst</name>
                <type EnumType="true">org.shredzone.commons.suncalc.SunTimes.Twilight</type>
            </parameter>
        </method>
        <method>
            <name>TwilightRise</name>
